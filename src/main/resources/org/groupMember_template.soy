{template .memberCreateOrUpdate}
    <div class="aui-page-panel-inner">
        <form name="form1" style="margin-top: 20px" class="aui form1">

            <div class="field-group">
                            <label>{getText('workorg.property.StrEmployee.role')}<span class="aui-icon icon-required">(required)</span></label>
                            <input type="hidden" id="roleId" name="roleId"/>
             </div>
            <div class="field-group">
                <label>{getText('workorg.property.StrEmployee.employeeNo')}</label>
                <input class="text medium-field" type="text" id="employeeNo" name="employeeNo"  />
            </div>
            <div class="field-group">
                <label>{getText('workorg.property.StrEmployee.employeeName')}<span class="aui-icon icon-required">(required)</span></label>
                <input class="text medium-field" type="text" id="employeeName" name="employeeName"  />
            </div>
            <div class="field-group">
                <label>{getText('workorg.property.StrEmployee.email')}</label>
                <input class="text medium-field" type="text" id="email" name="email"  />
            </div>


            <div class="field-group">
                <label>{getText('workorg.property.organization.org')}</label>
                <input type="hidden" id="orgId" name="orgId"/>
            </div>



            <div class="field-group">
                <label>{getText('workorg.property.StrEmployee.phone')}</label>
                <input class="text medium-field" type="text" id="phone" name="phone"  />
                <div class="description">{getText('workorg.property.form.valid.fillNumber')} + - ( ) * #</div>
            </div>

            <div class="field-group">
                <label>{getText('workorg.property.StrEmployee.employmentStatus')}</label>
                <select class="select" id="employmentStatus" name="employmentStatus" >
                    <option value="1">{getText('workorg.property.StrEmployee.onJob')}</option>
                    <option value="2">{getText('workorg.property.StrEmployee.dimission')}</option>
                </select>
            </div>


            <div class="buttons-container">
                <div class="buttons">
                    <input class="button" type="button" value='{getText('workorg.property.common.save')}' id="savebut" />
                    <span class="createAnother"><input type="checkbox" class="createAnotherCheckbox">create another</span>
                    <a class="cancel" href="#">{getText('workorg.property.common.cancel')}</a>
                </div>
            </div>
        </form>
    </div>
{/template}

