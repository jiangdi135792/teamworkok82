{namespace work.Org.Template}
/**
 * @param data
 */
{template .deptCreateAndUpdate}
    <div class="aui-page-panel-inner">
        <form class="aui form1">
            <div class="field-group">
                <label>{getText('workorg.property.Department.departName')}<span class="aui-icon icon-required">(required)</span></label>
                <input class="text medium-field" type="text" id="groupName" name="groupName"
                  value="{$data.groupName ? $data.groupName : ''}">
            </div>
            <div class="field-group">
                <label>{getText('workorg.property.Department.departOrTeamNo')}</label>
                <input class="text medium-field"
                    type="text" id="groupNo" name="groupNo" value="{$data.groupNo ? $data.groupNo: ''}">
            </div>
            <div class="field-group">
                <label>{getText('workorg.property.Department.ParentdepartName')}<span class="aui-icon icon-required">(required)</span></label>
//                <select class="select" id="parent" name="parent"></select>
                <input type="hidden" id="parent" name="parent">
            </div>
            <div class="field-group" style="display:none">
                <label>{getText('workorg.property.Department.status')}</label>
                <select class="select" id="status" name="status">
                    <option value="0" {if $data.status == 0}selected{/if}>{getText('workorg.property.Department.valid')}</option>
                    <option value="1" {if $data.status == 1}selected{/if}>{getText('workorg.property.Department.unvalid')}</option>
                </select>
            </div>
            <div class="field-group" style="display:none">
                <label>{getText('workorg.property.Department.type')}</label>
                <select class="select" id="type" name="type" >
                    <option value="0" {if $data.type == 0}selected{/if}{if $data.type == 1}disabled{/if}>{getText('workorg.property.Department.department')}</option>
                    <option value="1" {if $data.type == 1}selected{/if}{if $data.type == 0}disabled{/if}>{getText('workorg.property.Department.team')}</option>
                </select>
            </div>
//            <div class="field-group">
//                <label>{getText('workorg.property.Department.superior')}</label>
//                <input class="text medium-field" type="text"
//                       id="mappingCode" name="mappingCode"  value="{$data.mappingCode ? $data.mappingCode : ''}">
//            </div>
            <div class="field-group">
                <label for="comment-input">{getText('workorg.property.Department.remark')}</label>
                <textarea class="textarea" name="memo"
                          id="memo" placeholder="">{$data.memo ? $data.memo : ''}</textarea>
            </div>

            <div class="buttons-container">
                <div class="buttons">
                    <input class="button" type="button" value="{getText('workorg.property.Department.submit')}" id="save-button">
                    <a class="cancel" href="#">{getText('workorg.property.common.cancel')}</a>
                </div>
            </div>
        </form>
    </div>
{/template}

/**
 * @param data
 */
{template .orgCreateAndUpdate}
     <div class="aui-page-panel-inner">
        <form style="margin-top: 20px" class="aui form1"  >
            <input name="id" id="id" type="hidden">
            <div class="field-group">
                <label>
                    {getText('workorg.property.organization.name')}
                    <span class="aui-icon icon-required">(required)</span>
                </label>
                <input class="text medium-field" type="text" id="name" name="name" value="{$data.name ? $data.name : ''}">
            </div>
            <div class="field-group">
                            <label for="select-example">{getText('workorg.property.organization.parent')}</label>
                            <input type="hidden" id="parent" name="parent">
                        </div>
            <div class="field-group">
                <label>{getText('workorg.property.organization.type')}</label>
                <select class="select" id="type" name="type">
                    <option value="0" {if $data.type == 0}selected{/if}>{getText('workorg.property.organization.type.company')}</option>
                    <option value="1" {if $data.type == 1}selected{/if}>{getText('workorg.property.organization.type.subcompany')}</option>
                    <option value="2" {if $data.type == 2}selected{/if}>{getText('workorg.property.organization.type.branch')}</option>
                    <option value="9" {if $data.type == 9}selected{/if}>{getText('workorg.property.organization.type.other')}</option>
                </select>
            </div>
            <div class="field-group" style="display:none" id="characterDiv">
                <label for="select-example">{getText('workorg.property.organization.character')}</label>
                <select class="select" id="character" name="character">
                    <option value="0" {if $data.character == 0}selected{/if}>{getText('workorg.property.organization.character.generalsubsidiary')}</option>
                    <option value="1" {if $data.character == 1}selected{/if}>{getText('workorg.property.organization.character.independentlegal')}</option>
                </select>
            </div>
            <div class="field-group" style="display: none" id="statusDiv">
                <label for="select-example">{getText('workorg.property.organization.status')}</label>
                <select class="select" id="status" name="status">
                    <option value="0" {if $data.status == 0}selected{/if}>{getText('workorg.property.common.status.valid')}</option>
                    <option value="1" {if $data.status == 1}selected{/if}>{getText('workorg.property.common.status.invalid')}</option>
                </select>
            </div>
            <div class="field-group" style="display: none">
                <label for="select-example">{getText('workorg.property.organization.orgLevel')}</label>
                <select class="select" id="orgLevel" name="orgLevel">
                    <option value="1" {if $data.orgLevel == 0}selected{/if}>1</option>
                    <option value="2" {if $data.orgLevel == 2}selected{/if}>2</option>
                    <option value="3" {if $data.orgLevel == 3}selected{/if}>3</option>
                </select>
            </div>

            <div class="field-group">
                <label for="comment-input">{getText('workorg.property.organization.memo')}</label>
                <textarea class="textarea" name="memo" id="memo">{$data.memo ? $data.memo : ''}</textarea>
            </div>

            <div class="buttons-container">
                <div class="buttons">
                    <input class="button" type="button" value={getText('workorg.property.common.save')} id="save-button">
                    <a class="cancel" href="#">{getText('workorg.property.common.cancel')}</a>
                </div>
            </div>
        </form>
     </div>
{/template}

/**
@param data
@param pageSize
@param pageCount
@param currentPage
@param total
 */
{template .memberList}
   <div class="aui-help aui-help-text"  style="width:100%">
   <span class="aui-help-content" style="float:left;width:20%"><a>组织里人员列表</a></span>
       <span style="float:right;right:2px;width:30%"> <input id="searchEmployee" class="search" type="text" placeholder='{getText('workorg.property.StrEmployeeName')}|{getText('workorg.property.StrEmployee.employeeNo')}'>
        <button id="searchBtn"class="aui aui-button">{getText('workorg.property.common.search')}</button>
</span>
    </div>
    <table class="aui aui-table-sortable">
        <thead>
            <tr>
                <th>{getText('workorg.property.StrEmployee.employeeNo')}</th>
                <th>{getText('workorg.property.StrEmployee.employeeName')}</th>
                <th>{getText('workorg.property.issue.panel.Company')}</th>

                <th>{getText('workorg.property.StrEmployee.phone')}</th>
                <th>{getText('workorg.property.StrEmployee.employmentStatus')}</th>

                <th>{getText('workorg.property.StrEmployee.jiraUserName')}</th>
                <th>{getText('workorg.property.common.operation')}</th>
            </tr>
        </thead>
        <tbody>
            {foreach $item in $data}
                <tr data-employeeid='{$item.id}'>
                            <td>{$item.employeeNo ? $item.employeeNo : ''}</td>
                            <td headers="type">{$item.employeeName ? $item.employeeName : ''}</td>
                            <td headers="type">{$item.orgName ? $item.orgName : ''}</td>

                            <td>{$item.phone ? $item.phone : ''}</td>
                            <td>
                                {if $item.employmentStatus == 1}
                                    {getText('workorg.property.StrEmployee.onJob')}
                                {else}
                                    {getText('workorg.property.StrEmployee.dimission')}
                                {/if}
                            </td>

                            <td>{$item.jiraUserKey ? $item.jiraUserKey : ''}</td>
                            <td class="action" headers="action">
                                <ul class="menu">
                                    <li>
                                        <a class="edit" style="cursor: pointer;">{getText('workorg.property.common.edit')}</a>
                                    </li>
                                    <li>
                                        <a class="delete" style="cursor: pointer;">{getText('workorg.property.common.delete')}</a>
                                    </li>
                                </ul>
                            </td>
                        </tr>
            {ifempty}
            <tr><td colspan="9" style="text-align:center">{getText('workorg.property.StrEmployee.nomember')}<td><tr>
            {/foreach}
        </tbody>
    </table>
    <div style="margin-right: 20px;position: relative; margin-top:16px;">
        <span>{getText('workorg.property.organization.page.desc1')}{$total}{getText('workorg.property.organization.page.desc2')}</span>
        <select id="pageCount">
            <option value="5"   {if $pageCount == 5}  selected{/if}>5</option>
            <option value="10"  {if $pageCount == 10} selected{/if}>10</option>
            <option value="20"  {if $pageCount == 20} selected{/if}>20</option>
            <option value="50"  {if $pageCount == 50} selected{/if}>50</option>
            <option value="1000"  {if $pageCount == 1000} selected{/if}>1000</option>
        </select>
        <span>{getText('workorg.property.organization.page.desc3')}</span>

        /**
        *  �??  �??, 跳转到第  �??
        */
        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        {getText('workorg.property.organization.page.desc1')}{$pageSize}{getText('workorg.property.organization.page.desc4')}
        <input id="skipToInput" type="text" onkeyup="this.value=this.value.replace(/[^\d]/g,'') " onafterpaste="this.value=this.value.replace(/[^\d]/g,'') " size="2" />
        {getText('workorg.property.organization.page.desc5')}&nbsp;&nbsp;
        <input type="button" id="skipToButton" value="{getText('workorg.property.common.confim')}"/>
        <input id="pageSize" type="text" hidden="hidden" value={$pageSize}/>
       <div style="position:absolute;right:0;top:0;">
        /**
        * 上一�??
        */
        {if $currentPage == 1}
            <font size="2"> {getText('workorg.property.organization.page.desc6')}</font>
        {/if}
        {if $currentPage > 1}
            <a class="pageNo" style="cursor: pointer; margin-left:5px;"><font size="2">{getText('workorg.property.organization.page.desc6')}</font></a>
        {/if}
        &nbsp;&nbsp;&nbsp;&nbsp;
        /**
        * 只显�?? �?? n-5 �?? ~ �?? n+4 �??
        */
        {if $currentPage -5 >= 1}
            {if $currentPage +4 <= $pageSize}
                {for $i in range($currentPage - 6, $currentPage + 4)}
                    {if $i + 1 == $currentPage}
                        &nbsp;&nbsp;<strong id='currentPageNo'>{$i + 1}</strong>&nbsp;
                    {/if}
                    {if $i + 1 != $currentPage}
                        &nbsp;<a class="pageNo" style="cursor: pointer; margin-left:5px;">{$i + 1}</a>&nbsp;
                    {/if}
                {/for}
            {/if}
            {if $currentPage +4 > $pageSize}
                {for $i in range($currentPage - 6, $pageSize)}
                    {if $i + 1 == $currentPage}
                        &nbsp;&nbsp;<strong id='currentPageNo'>{$i + 1}</strong>&nbsp;
                    {/if}
                    {if $i + 1 != $currentPage}
                        &nbsp;<a class="pageNo" style="cursor: pointer; margin-left:5px;">{$i + 1}</a>&nbsp;
                    {/if}
                {/for}
            {/if}
        {/if}
        {if $currentPage -5 < 1}
            {if $currentPage +4 <= $pageSize}
                {for $i in range(0, $currentPage + 4)}
                    {if $i + 1 == $currentPage}
                        &nbsp;&nbsp;<strong id='currentPageNo'>{$i + 1}</strong>&nbsp;
                    {/if}
                    {if $i + 1 != $currentPage}
                        &nbsp;<a class="pageNo" style="cursor: pointer; margin-left:5px;">{$i + 1}</a>&nbsp;
                    {/if}
                {/for}
            {/if}
            {if $currentPage +4 > $pageSize}
                {for $i in range(0, $pageSize)}
                    {if $i + 1 == $currentPage}
                        &nbsp;&nbsp;<strong id='currentPageNo'>{$i + 1}</strong>&nbsp;
                    {/if}
                    {if $i + 1 != $currentPage}
                        &nbsp;<a class="pageNo" style="cursor: pointer; margin-left:5px;">{$i + 1}</a>&nbsp;
                    {/if}
                {/for}
            {/if}
        {/if}
        &nbsp;&nbsp;&nbsp;&nbsp;
        /**
        * 下一�??
        */
        {if $currentPage == $pageSize}
            <font size="2">{getText('workorg.property.organization.page.desc7')}</font>
        {/if}
        {if $currentPage < $pageSize}
            <a class="pageNo" style="cursor: pointer; margin-left:5px;"><font size="2">{getText('workorg.property.organization.page.desc7')}</font></a>
        {/if}
        </div>
    </div>
{/template}

